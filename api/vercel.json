{
    "version": 2,
    "name": "your-app-name",
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node",
        "config": {
            "includeFiles": ["db.json"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ]
}